@page
@model IndexModel

<h2>Dynamic Form</h2>

<form method="post">
    @for (int i = 0; i < Model._questionService.questions.Length; i++) //added ? for nullable to remove exception
    {
        <div>
            <label for="question_@i">@Model._questionService.questions[i]</label>
            <input type="text" id="question_@i" name="question_@i" />
        </div>
    }
    <button type="submit">Submit</button>
</form>


